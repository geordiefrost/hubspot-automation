{
  "name": "hubspot-automation-platform",
  "version": "1.0.0",
  "description": "HubSpot setup automation platform for Bang Digital",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint server/ client/src/",
    "lint:fix": "eslint server/ client/src/ --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "sequelize": "^6.32.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "crypto": "^1.0.1",
    "axios": "^1.5.0",
    "multer": "^1.4.5-lts.1",
    "papaparse": "^5.4.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "redis": "^4.6.7",
    "rate-limiter-flexible": "^3.0.8",
    "node-cron": "^3.0.2",
    "express-rate-limit": "^6.10.0",
    "express-slow-down": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "sequelize-cli": "^6.6.1",
    "eslint": "^8.46.0",
    "eslint-plugin-node": "^11.1.0",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "hubspot",
    "automation",
    "crm",
    "setup",
    "properties",
    "pipelines"
  ],
  "author": "Bang Digital",
  "license": "MIT"
}